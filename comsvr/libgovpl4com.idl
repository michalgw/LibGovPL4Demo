    helpstring("Tworzenie instancji obiektu obslugi KSeF")]
    HRESULT _stdcall CreateKSeF([out, retval] IDispatch** NewObject);

    [id(2015)]
    HRESULT _stdcall CreateXMLReader([in] BSTR ClsName, [in] VARIANT Source, [out, retval] IDispatch** NewObject);

    [id(2016)]
    HRESULT _stdcall CreateXMLValidator([in] BSTR ClsName, [out, retval] IDispatch** NewObj);

    [id(2017)]
    HRESULT _stdcall CreateXMLXSLTranformation([in] BSTR ClsName, [out, retval] IDispatch** NewObject);

    ///
    /// Tworzenie instacji obiektu klienta KSeF.
    /// @return instancja obiektu implementującego interfejs IlgcKSeF2.
    ///
    [id(2019),
    helpstring("Tworzenie instancji obiektu obslugi KSeF 2.0")]
    HRESULT _stdcall CreateKSeF2([out, retval] IDispatch** NewObject);

    [id(2020)]
    HRESULT _stdcall CreateHash([in] BSTR ClsName, [out, retval] IDispatch** NewObject);

    [id(2021)]
